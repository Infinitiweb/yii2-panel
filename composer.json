{
  "name": "infinitiweb/yii2-panel",
  "description": "Yii2 panel widget",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "panel bootstrap",
    "panel for yii2",
    "widget"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tutik Alexsandr",
      "email": "sanchezzzhak@ya.ru"
    },
    {
      "name": "Ruslan Zhovnerovskiy",
      "email": "hello.omny@gmail.com"
    }
  ],
  "config": {
    "process-timeout": 1800,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": ">=2.0.12",
    "bower-asset/font-awesome": ">4.*@stable",
    "bower-asset/slimScroll": ">=1.3.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "infinitiweb\\widgets\\yii2\\panel\\": "src"
    }
  }
}